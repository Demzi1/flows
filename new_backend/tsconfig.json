{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./src",
    "baseUrl": "./",
    "paths": {
        "@/*":["src/*"],
        "@generated/*":["generated/*"]
    },
    "strict": false,
    "allowImportingTsExtensions": true,
    "noEmit":true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*"],
}